{

  "manifest_version": 2,
  "name": "Improved Shopify Admin Bar",
  "version": "1.0",

  "description": "Adds additoinal functionality to admin bar",

  "permissions": [
      "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery-3.3.1.js","improved-shopify-admin-bar.js"],
      "css": ["css/improved-shopify-admin-bar.css"]
    }
  ]

}